<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="video_progressbar_style">
        <item name="android:maxWidth">43dp</item>
        <item name="android:maxHeight">43dp</item>
        <item name="android:indeterminateDrawable">@drawable/loading_progressbar_play_video</item>
        <item name="android:minWidth">43dp</item>
        <item name="android:minHeight">43dp</item>
    </style>

</resources>
